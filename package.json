{
  "name": "looks-to-me",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.6.6",
  "scripts": {
    "dev": "next dev",
    "dev:storybook": "storybook dev",
    "dev:pages": "next-on-pages --watch",
    "build": "next build",
    "build:storybook": "storybook build -o public/storybook && mv public/storybook/index.html public/storybook/index.htm",
    "build:pages": "next-on-pages",
    "build:ci": "pnpm run build:storybook && pnpm run build:pages",
    "start": "next start",
    "start:pages": "wrangler pages dev .vercel/output/static --compatibility-flag=nodejs_compat"
  },
  "pnpm": {
    "overrides": {
      "tsconfig-paths": "4.2.0"
    },
    "patchedDependencies": {
      "esbuild-register@3.4.2": "patches/esbuild-register@3.4.2.patch"
    }
  },
  "dependencies": {
    "@radix-ui/react-slot": "1.0.2",
    "clsx": "1.2.1",
    "modern-normalize": "2.0.0",
    "next": "13.4.9",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "1.2.0",
    "@storybook/addon-essentials": "7.0.26",
    "@storybook/addon-interactions": "7.0.26",
    "@storybook/addon-links": "7.0.26",
    "@storybook/builder-webpack5": "7.0.26",
    "@storybook/nextjs": "7.0.26",
    "@storybook/react": "7.0.26",
    "@tsconfig/next": "2.0.0",
    "@tsconfig/strictest": "2.0.1",
    "@types/node": "20.4.1",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@typescript-eslint/eslint-plugin": "5.61.0",
    "@vanilla-extract/css": "1.12.0",
    "@vanilla-extract/css-utils": "0.1.3",
    "@vanilla-extract/next-plugin": "2.1.3",
    "@vanilla-extract/recipes": "0.4.0",
    "@vanilla-extract/webpack-plugin": "2.2.0",
    "eslint": "8.44.0",
    "eslint-config-next": "13.4.9",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.2",
    "eslint-plugin-unused-imports": "2.0.0",
    "mini-css-extract-plugin": "2.7.6",
    "storybook": "7.0.26",
    "typescript": "5.1.6",
    "vercel": "30.2.3",
    "webpack-merge": "5.9.0",
    "wrangler": "3.1.1"
  }
}
