{
  "name": "looks-to-me",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.6.4",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "next-on-pages",
    "pages:watch": "next-on-pages --watch",
    "pages:dev": "wrangler pages dev .vercel/output/static --compatibility-flag=nodejs_compat",
    "pages:deploy": "run pages:build && wrangler pages publish .vercel/output/static"
  },
  "pnpm": {
    "overrides": {
      "tsconfig-paths": "4.2.0"
    }
  },
  "dependencies": {
    "clsx": "1.2.1",
    "modern-normalize": "2.0.0",
    "next": "13.4.7",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "1.1.0",
    "@tsconfig/next": "1.0.5",
    "@tsconfig/strictest": "2.0.1",
    "@types/node": "20.3.1",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@typescript-eslint/eslint-plugin": "5.60.0",
    "@vanilla-extract/css": "1.12.0",
    "@vanilla-extract/css-utils": "0.1.3",
    "@vanilla-extract/next-plugin": "2.1.3",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.7",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.2",
    "eslint-plugin-unused-imports": "2.0.0",
    "typescript": "5.1.3",
    "vercel": "30.2.3",
    "wrangler": "3.1.1"
  }
}
