{
  "name": "looks-to-me",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.6.10",
  "scripts": {
    "create:component": "scaffdog generate component",
    "dev": "next dev",
    "dev:proxy": "cf-bindings-proxy",
    "dev:storybook": "storybook dev",
    "build": "next build",
    "build:storybook": "storybook build -o public/storybook && mv public/storybook/index.html public/storybook/index.htm",
    "build:pages": "next-on-pages",
    "build:ci": "pnpm run build:storybook && pnpm run build:pages",
    "lint:code": "next lint",
    "lint:type": "tsc --pretty --noEmit",
    "migrate:create": "drizzle-kit generate:sqlite",
    "migrate:apply": "wrangler d1 migrations apply local --local"
  },
  "pnpm": {
    "overrides": {
      "tsconfig-paths": "4.2.0"
    },
    "patchedDependencies": {
      "esbuild-register@3.4.2": "patches/esbuild-register@3.4.2.patch"
    }
  },
  "dependencies": {
    "@radix-ui/react-accessible-icon": "1.0.3",
    "@radix-ui/react-dialog": "1.0.4",
    "@radix-ui/react-separator": "1.0.3",
    "@radix-ui/react-slot": "1.0.2",
    "@radix-ui/react-tooltip": "1.0.6",
    "@radix-ui/react-visually-hidden": "1.0.3",
    "@t3-oss/env-nextjs": "0.6.0",
    "cf-bindings-proxy": "0.2.4",
    "clsx": "2.0.0",
    "deepmerge": "4.3.1",
    "drizzle-orm": "0.27.2",
    "modern-normalize": "2.0.0",
    "next": "13.4.12",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "1.4.0",
    "@cloudflare/workers-types": "4.20230724.0",
    "@scaffdog/types": "2.5.1",
    "@storybook/addon-essentials": "7.1.1",
    "@storybook/addon-interactions": "7.1.1",
    "@storybook/addon-links": "7.1.1",
    "@storybook/builder-webpack5": "7.1.1",
    "@storybook/nextjs": "7.1.1",
    "@storybook/react": "7.1.1",
    "@svgr/webpack": "8.0.1",
    "@tsconfig/next": "2.0.0",
    "@tsconfig/strictest": "2.0.1",
    "@types/node": "20.4.5",
    "@types/react": "18.2.17",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "@vanilla-extract/css": "1.12.0",
    "@vanilla-extract/css-utils": "0.1.3",
    "@vanilla-extract/next-plugin": "2.2.1",
    "@vanilla-extract/recipes": "0.5.0",
    "@vanilla-extract/webpack-plugin": "2.2.0",
    "better-sqlite3": "8.5.0",
    "drizzle-kit": "0.19.12",
    "eslint": "8.45.0",
    "eslint-config-next": "13.4.12",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-next-on-pages": "1.4.0",
    "eslint-plugin-unused-imports": "3.0.0",
    "mini-css-extract-plugin": "2.7.6",
    "scaffdog": "2.5.1",
    "storybook": "7.1.1",
    "typescript": "5.1.6",
    "vercel": "30.2.3",
    "webpack-merge": "5.9.0",
    "wrangler": "3.4.0"
  }
